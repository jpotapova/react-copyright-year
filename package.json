    "react-dom": "^15.6.2"
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-15": "^1.4.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
